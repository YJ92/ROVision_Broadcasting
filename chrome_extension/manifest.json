{
	"name" : "ROVision Screen Capture",
	"version" : "0.0.1",
	"manifest_version" : 2,
	"browser_action" : {
		"default_icon" : "img/tabCapture128.png"
	},
	"background" : {
		"scripts" : ["js/adapter.js","js/socket.io.js","js/background.js"],
		"persistent" : true
	},
	"permissions": [
		"activeTab",
		"tabCapture"
	]
}
